        <Button id="wishlistButton" icon="sap-icon://favorite" type="Transparent" class="greenButton" press=".onWishlistPress"/>
        <Button id="cartButton" icon="sap-icon://cart" type="Transparent" class="greenButton" press=".onCartPress"/>
        <Button id="accountButton" icon="sap-icon://customer" type="Transparent" class="greenButton" press=".onAccountPress"/>
        <ToolbarSpacer id="_IDGenToolbarSpacer"/>
    </Toolbar>

</core:FragmentDefinition>
